}
else
{
if (disc > 0) # if2, two roots
{
root1 <- (-bb - sqrt(disc)) / (2 * aa)
root2 <- (-bb + sqrt(disc)) / (2 * aa)
return(c(root1, root2))
}
else # no real roots, return NA
{
return("pas de solution dans R")
}
}
}
